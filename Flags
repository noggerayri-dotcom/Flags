-- Apply all flags
for name, value in pairs(flags) do
    if type(value) == "boolean" then
        pcall(function()
            game:SetFastFlag(name, value)
        end)
    elseif type(value) == "number" then
        pcall(function()
            game:SetFastInt(name, value)
        end)
    end
end

print("All FastFlags and FInts applied successfully!")
